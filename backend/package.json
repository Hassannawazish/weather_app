{
    "name": "weather-api",
    "version": "1.0.0",
    "description": "Weather Data API using Node.js and Express",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "lint": "eslint ./**/*.js",
        "build": "npm run lint && npx babel . --out-dir dist && npm run optimize",
        "optimize": "npx terser dist/*.js --output dist/optimized.js"
    },
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.17.1",
        "mongoose": "^5.12.3"
    },
    "devDependencies": {
        "nodemon": "^2.0.7",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-preset-env": "^1.7.0",
        "eslint": "^8.26.0",
        "terser": "^5.12.1"
    }
}
